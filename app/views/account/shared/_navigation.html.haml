%ul.tabs(data-tabs="tabs")
  %li{:class => @main_nav == :dashboard ? :active : nil}
    = link_to "Dashboard", dashboard_path
  %li{:class => @main_nav == :profile ? :active : nil}
    = link_to "Profile", edit_profile_path
  %li{:class => @main_nav == :messages ? :active : nil}
    = link_to "Messages", [:account, :messages]
  %li{:class => @main_nav == :notification_center ? :active : nil}
    = link_to "Notifications Center", account_notifications_center_path

