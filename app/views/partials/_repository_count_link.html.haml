/ repository

= link_to repository.watchers_count,
          user_repository_watchers_path(repository.username, repository.name),
          :class => "btn large disabled",
          :title => t("web.page.repository_watchers"),
          :data => { "repository_watchers_count_#{repository.id}" => true }


= link_to repository.forks_count,
          user_repository_forks_path(repository.username, repository.name),
          :class => "btn large disabled",
          :title => t("web.page.repository_forks"),
          :data => { "repository_forks_count_#{repository.id}" => true }

